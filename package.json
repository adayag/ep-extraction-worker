{
  "name": "ep-extraction-worker",
  "version": "1.0.0",
  "description": "Remote m3u8 extraction worker for EP Live Events",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:browser": "tsx scripts/test-extract.ts",
    "test:browser:interactive": "tsx scripts/test-extract.ts --interactive",
    "test:browser:verbose": "tsx scripts/test-extract.ts --verbose"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "express": "^4.21.0",
    "p-queue": "^9.1.0",
    "patchright": "^1.57.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.10",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.2.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
